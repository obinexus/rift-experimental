# RIFT Stage 5 Governance Configuration
# Schema Version: 1.0

[general]
stage = 5
version = 0.1.0-alpha

[validation]
yoda_style = required
null_nil_semantics = enforce
thread_lifecycle = validate
parity_elimination = enable

[audit]
audit_5_stdin = true
audit_5_stderr = true
audit_5_stdout = true
state_hash = required

[telemetry]
stage_5_tracing = enable
guid_generation = required
uuid_generation = required
crypto_hash = sha256
prng_seed = time_based
